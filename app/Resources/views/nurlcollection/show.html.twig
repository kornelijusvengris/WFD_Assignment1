{% extends '_base.html.twig' %}

{% block body %}
    <h1>Nurlcollection</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ nurlCollection.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ nurlCollection.title }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('nurlcollection_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('nurlcollection_edit', { 'id': nurlCollection.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
